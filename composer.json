{
    "name": "elsed115/resource-file-manager",
    "description": "A Laravel Nova file manager tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Elsed\\ResourceFileManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elsed\\ResourceFileManager\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    "post-install-cmd": [
        "php artisan storage:link", 
        "ln -s vendor/elsed115/resource-file-manager/public/svg public/svg",
        "composer require league/flysystem-aws-s3-v3 '^3.0' --with-all-dependencies"

    ],
    "post-update-cmd": [
        "php artisan storage:link", 
        "ln -s vendor/elsed115/resource-file-manager/public/svg public/svg",
        "composer require league/flysystem-aws-s3-v3 '^3.0' --with-all-dependencies"

    ]
}
}
